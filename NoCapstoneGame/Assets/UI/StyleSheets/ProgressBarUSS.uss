.unity-progress-bar {
}

.unity-progress-bar__container {
    flex-grow: 1;
}

.unity-progress-bar__progress {
}

.unity-progress-bar__background {
    height: 100%;
    background-color: rgba(0, 0, 0, 0.55);
}

.unity-vertical-progress-bar {
    flex-shrink: 0;
    margin-bottom: 3px;
    margin-left: 1px;
    margin-top: 3px;
    margin-right: 1px;
    background-color: rgb(76, 82, 106);
    -unity-text-outline-color: rgb(48, 48, 48);
}

.unity-vertical-progress-bar__container {
    flex-grow: 1;
    background-color: rgba(76, 82, 106, 0);
}

.unity-vertical-progress-bar__progress {
    background-color: rgba(255, 255, 255, 0);
    position: absolute;
    right: 0;
    bottom: 0;
    left: 0;
}

.unity-vertical-progress-bar__background {
    background-color: rgba(0, 0, 0, 0);
    width: 100%;
    border-left-width: 1px;
    border-right-width: 1px;
    border-top-width: 1px;
    border-bottom-width: 1px;
    border-left-color: rgb(37, 37, 37);
    border-right-color: rgb(37, 37, 37);
    border-top-color: rgb(37, 37, 37);
    border-bottom-color: rgb(37, 37, 37);
    height: 100%;
}
